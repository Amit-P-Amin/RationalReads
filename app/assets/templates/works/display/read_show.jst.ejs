<td>
  <p class="title">
    <a href="#/works/<%= work.escape("id") %>"> <%= work.escape("name") %> </a>
  </p>
</td>
<td>
  <p class="author">
    <%= work.escape("author") %>
  </p>
</td>
<td>
  <p class="length">
    <%= work.escape("length") %>
  </p>
</td>
<td class="avg-rating" title="Bayesian Average Rating">
  <p class="avg">
    <%= parseFloat(work.escape("bayesian_average")).toFixed(2) %>
  </p>
</td>
<td class="num-ratings">
  <p class="num-ratings-number">
<%= work.escape("ratings") %>
  </p>
</td>
<td class="stars">
  <p class="hidden"><%= this.model.get("user_rating") %></p> <div class="raty"></div>
</td>
